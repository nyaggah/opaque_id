{
  "release-type": "ruby",
  "packages": {
    ".": {
      "package-name": "opaque_id",
      "version-file": "lib/opaque_id/version.rb"
    }
  },
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features"
    },
    {
      "type": "fix",
      "section": "Bug Fixes"
    },
    {
      "type": "perf",
      "section": "Performance Improvements"
    },
    {
      "type": "revert",
      "section": "Reverts"
    },
    {
      "type": "docs",
      "section": "Documentation"
    },
    {
      "type": "style",
      "section": "Styles"
    },
    {
      "type": "chore",
      "section": "Miscellaneous Chores"
    },
    {
      "type": "refactor",
      "section": "Code Refactoring"
    },
    {
      "type": "test",
      "section": "Tests"
    },
    {
      "type": "build",
      "section": "Build System"
    },
    {
      "type": "ci",
      "section": "Continuous Integration"
    }
  ],
  "changelog-type": "default",
  "bump-minor-pre-major": true,
  "bump-patch-for-minor-pre-major": true,
  "draft": false,
  "prerelease": false,
  "include-component-in-tag": false,
  "include-v-in-tag": true,
  "pull-request-title-pattern": "chore: release ${version}",
  "pull-request-header": "Release ${version}",
  "pull-request-footer": "This PR was automatically created by Release Please. See [documentation](https://github.com/googleapis/release-please) for more info.",
  "separate-pull-requests": false,
  "sequential-calls": true,
  "skip-github-release": false,
  "versioning": "default"
}
